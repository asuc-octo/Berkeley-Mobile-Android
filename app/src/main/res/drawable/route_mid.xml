<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <size
                android:height="96dp"
                android:width="32dp" />
        </shape>
    </item>

    <item
        android:top="0dp"
        android:bottom="0dp"
        android:left="14.5dp"
        android:right="14.5dp" >
        <shape android:shape="rectangle">
            <solid
                android:color="@color/ASUC_blue" />
        </shape>
    </item>

    <item
        android:top="36dp"
        android:right="4dp"
        android:left="4dp"
        android:bottom="36dp" >
        <shape android:shape="oval">
            <solid
                android:color="@color/ASUC_blue" />
            <stroke
                android:width="3dp"
                android:color="@color/ASUC_blue" />
        </shape>
    </item>

</layer-list>